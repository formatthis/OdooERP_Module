<odoo>
    <template id="report_customer_health_training_card">
        <t t-call="web.html_container">
            <t t-foreach="docs" t-as="doc">
                <div class="page">
                    <h2>Customer Health Training Report</h2>
                    <p><strong>Name:</strong> <t t-esc="doc.name"/></p>
                    <p><strong>Phone:</strong> <t t-esc="doc.phone"/></p>
                    <p><strong>Email:</strong> <t t-esc="doc.email"/></p>
                    <p><strong>Address:</strong> <t t-esc="doc.address"/></p>
                    <p><strong>Chronic Diseases:</strong> <t t-esc="doc.chronic_diseases"/></p>
                </div>
            </t>
        </t>
    </template>
</odoo>